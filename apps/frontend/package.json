{
  "name": "@memalerts/web",
  "_deploy_trigger": "beta-trigger-2025-12-27-2",
  "version": "1.0.10",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:overlay": "cd overlay && vite",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc -p tsconfig.prod.json --noEmit",
    "typecheck:all": "tsc --noEmit",
    "typecheck:overlay": "cd overlay && tsc --noEmit",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "build:web": "tsc -p tsconfig.prod.json && vite build",
    "build:overlay": "cd overlay && tsc && vite build",
    "preview": "vite preview",
    "preview:web": "vite preview",
    "preview:overlay": "cd overlay && vite preview",
    "lint": "eslint src overlay --ext ts,tsx --report-unused-disable-directives --ignore-pattern \"**/*.test.*\" --ignore-pattern \"**/*.spec.*\" --ignore-pattern \"src/test/**\" --ignore-pattern \"overlay/test/**\"",
    "lint:strict": "eslint src overlay --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern \"**/*.test.*\" --ignore-pattern \"**/*.spec.*\" --ignore-pattern \"src/test/**\" --ignore-pattern \"overlay/test/**\"",
    "lint:all": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:overlay": "cd overlay && eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:overlay:strict": "cd overlay && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^25.0.1",
    "msw": "^2.11.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
