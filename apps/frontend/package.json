{
  "name": "@memalerts/frontend",
  "_deploy_trigger": "beta-trigger-2025-12-27-2",
  "version": "1.0.25",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:overlay": "cd overlay && vite",
    "test": "vitest",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc -p tsconfig.prod.json --noEmit",
    "typecheck:all": "tsc --noEmit",
    "typecheck:overlay": "cd overlay && tsc --noEmit",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "build:web": "tsc -p tsconfig.prod.json && vite build",
    "build:overlay": "cd overlay && tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:web": "vite preview",
    "preview:overlay": "cd overlay && vite preview",
    "lint": "eslint src overlay --ext ts,tsx --report-unused-disable-directives --ignore-pattern \"**/*.test.*\" --ignore-pattern \"**/*.spec.*\" --ignore-pattern \"src/test/**\" --ignore-pattern \"overlay/test/**\"",
    "lint:strict": "eslint src overlay --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern \"**/*.test.*\" --ignore-pattern \"**/*.spec.*\" --ignore-pattern \"src/test/**\" --ignore-pattern \"overlay/test/**\"",
    "lint:all": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:overlay": "cd overlay && eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:overlay:strict": "cd overlay && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@memalerts/api-contracts": "workspace:*",
    "@reduxjs/toolkit": "^2.11.2",
    "@tanstack/react-query": "^5.45.1",
    "axios": "^1.13.2",
    "i18next": "^23.16.8",
    "zod": "^3.22.0",
    "i18next-browser-languagedetector": "^7.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^14.1.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.3",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.30",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jsdom": "^25.0.1",
    "msw": "^2.12.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  }
}
